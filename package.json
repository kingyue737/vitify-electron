{
  "private": true,
  "packageManager": "pnpm@7.29.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "test:unit": "vitest",
