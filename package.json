    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-router": "^0.5.4",
    "vite": "^4.2.0",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-vue-layouts": "^0.8.0",
